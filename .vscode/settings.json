{
	"editor.codeActionsOnSave": {
		"source.fixAll.biome": "explicit",
		"source.organizeImports.biome": "explicit"
	},
	"editor.defaultFormatter": "biomejs.biome",
	"CodeGPT.apiKey": "Gemini CLI",
	"Codegeex.Chat.LanguagePreference": "English",
	"Codegeex.Comment.LanguagePreference": "English",
	"Codegeex.CommitMessageStyle": "ConventionalCommits",
	"Codegeex.CommitMessage.LanguagePreference": "English",
	"Codegeex.GenerationPreference": "automatic",
	"Codegeex.SidebarUI.LanguagePreference": "English",
	"Codegeex.RepoIndex": true,
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"zencoder.mcpServers": {
		"ShadCN_Tweak": {
			"command": "npx",
			"args": [
				"-y",
				"shadcn@canary",
				"registry:mcp"
			],
			"env": {
				"REGISTRY_URL": "https://tweakcn.com/r/themes/registry.json"
			}
		}
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	}
}
